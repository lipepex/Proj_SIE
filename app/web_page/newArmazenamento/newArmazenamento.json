    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.armazenamento ){",
        "\treturn {",
        "\t\t//map armazenamento variable to expected task contract input",
        "\t\tarmazenamentoInput: {",
        "\t\t\tid_produto: $data.armazenamento.id_produto !== undefined ? $data.armazenamento.id_produto : null,",
        "\t\t\tetiquetaS: $data.armazenamento.etiquetaS !== undefined ? $data.armazenamento.etiquetaS : null,",
        "\t\t\tarmazenarStock: $data.armazenamento.armazenarStock !== undefined ? $data.armazenamento.armazenarStock : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
